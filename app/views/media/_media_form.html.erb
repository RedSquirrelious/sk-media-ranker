<%= @error if @error %>
<%= form_for @my_medium, method: @post_method, url: @post_path  do |f| %>

	<%= f.hidden_field :kind, value: params[:kind] %>

	<%= f.label :title %>
	<%= f.text_field :title %>

	<%= f.label :maker %>
	<%= f.text_field :maker %>

	<%= f.label :description %>
	<%= f.text_area :description %>

	<%= f.submit "Submit Art" %>
<% end %>
